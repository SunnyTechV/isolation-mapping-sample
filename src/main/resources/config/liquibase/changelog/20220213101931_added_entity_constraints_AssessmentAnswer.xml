<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">
    <!--
        Added the constraints for entity AssessmentAnswer.
    -->
    <changeSet id="20220213101931-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="assessment_id"
                                 baseTableName="assessment_answer"
                                 constraintName="fk_assessment_answer__assessment_id"
                                 referencedColumnNames="id"
                                 referencedTableName="assessment"/>

        <addForeignKeyConstraint baseColumnNames="question_id"
                                 baseTableName="assessment_answer"
                                 constraintName="fk_assessment_answer__question_id"
                                 referencedColumnNames="id"
                                 referencedTableName="question"/>
    </changeSet>
</databaseChangeLog>
